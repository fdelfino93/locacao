export const API_CONFIG = {
  BASE_URL: import.meta.env.VITE_API_BASE_URL || 'http://192.168.1.159:8080',
  API_BASE_URL: `${import.meta.env.VITE_API_BASE_URL || 'http://192.168.1.159:8080'}/api`
};

export const getApiUrl = (endpoint: string): string => {
  return `${API_CONFIG.API_BASE_URL}${endpoint}`;
};